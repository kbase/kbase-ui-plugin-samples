        "@types/react": "16",
        "@types/react-dom": "16",
        "@types/react-redux": "7",
        "ajs": "1.3.3",
        "antd": "4.14.1",
        "babel-plugin-import": "1.13.3",
        "customize-cra": "1.0.0",
        "leaflet": "1.7.1",
        "less": "4.1.1",
        "less-loader": "7.3.0",
        "nodemon": "2.0.7",
        "react": "16.14.0",
        "react-app-rewired": "2.1.8",
        "react-dom": "16.14.0",
        "react-dom-server": "0.0.5",
        "react-intl": "5.15.3",
        "react-leaflet": "3.1.0",
