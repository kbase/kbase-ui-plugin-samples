    },
    "devDependencies": {
        "redux-mock-store": "1.5.4"
    },
    "scripts": {
        "start": "nodemon -w ./config-overrides.js --ext ts,tsx,css,yml,yaml,json --exec 'react-app-rewired start'",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test",
        "test-coverage": "react-app-rewired test --coverage --watchAll=false"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "./",
    "proxy": "https://ci.kbase.us",
    "jest": {
        "collectCoverageFrom": [
            "./src/**/*.{ts,tsx}",
            "!/node_modules/",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ]
    }
}
