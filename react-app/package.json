    "@types/leaflet": "1",
    "@types/react": "16",
    "@types/react-dom": "16",
    "@types/react-redux": "7",
    "@types/react-router-dom": "5",
    "ajs": "1.3.3",
    "antd": "4.15.0",
    "leaflet": "1.7.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-intl": "5.15.7",
    "react-leaflet": "3.1.0",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@jedmao/redux-mock-store": "3.0.5",
    "@testing-library/dom": "7.30.3",
    "@testing-library/jest-dom": "5.11.10",
    "@testing-library/react": "11.2.6",
    "@testing-library/user-event": "13.1.1",
    "@types/jest": "26",
    "@types/node": "14",
    "babel-plugin-import": "1.13.3",
    "customize-cra": "1.0.0",
    "jest-fetch-mock": "3.0.3",
    "less": "4.1.1",
    "less-loader": "7.3.0",
    "nodemon": "2.0.7",
    "react-app-rewired": "2.1.8",
    "redux-mock-store": "1.5.4",
    "typescript": "4.2.4"
  },
  "scripts": {
    "start": "nodemon -w ./config-overrides.js --ext ts,tsx,css,yml,yaml,json --exec 'react-app-rewired start'",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "test-coverage": "react-app-rewired test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": "./",
  "proxy": "https://ci.kbase.us",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!src/lib/ViewModel/patch/*.*"
    ]
  }
}
