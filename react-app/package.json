{
    "name": "react-app",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@ant-design/icons": "4.2.1",
        "@kbase/ui-components": "0.2.1-alpha.20",
        "@kbase/ui-lib": "0.2.1-alpha.3",
        "@types/jest": "*",
        "@types/leaflet": "*",
        "@types/node": "*",
        "@types/react": "*",
        "@types/react-dom": "*",
        "@types/react-leaflet": "*",
        "@types/react-redux": "*",
        "antd": "4.3.1",
        "babel-plugin-import": "1.13.0",
        "customize-cra": "1.0.0",
        "jest-environment-jsdom-sixteen": "1.0.3",
        "leaflet": "1.6.0",
        "less": "3.11.2",
        "less-loader": "6.1.0",
        "react": "16.13.1",
        "react-app-rewired": "2.1.6",
        "react-dom": "16.13.1",
        "react-dom-server": "0.0.5",
        "react-leaflet": "2.7.0",
        "react-redux": "7.2.0",
        "react-scripts": "3.4.1",
        "redux": "4.0.5",
        "redux-thunk": "2.3.0"
    },
    "scripts": {
        "start": "nodemon -w ./config-overrides.js --exec 'react-app-rewired start'",
        "startx": "react-app-rewired start",
        "build": "react-app-rewired build",
        "test": "react-app-rewired test --coverage --watchAll=false  --env=jest-environment-jsdom-sixteen",
        "test2": "react-scripts test"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "homepage": "./",
    "proxy": "https://ci.kbase.us",
    "devDependencies": {
        "@testing-library/dom": "7.9.0",
        "@testing-library/jest-dom": "5.9.0",
        "@testing-library/react": "10.0.5",
        "@testing-library/user-event": "11.0.0",
        "nodemon": "2.0.4",
        "typescript": "3.9.3"
    },
    "jest": {
        "collectCoverageFrom": [
            "./src/**/*.{ts,tsx}",
            "!/node_modules/",
            "!src/index.tsx",
            "!src/serviceWorker.ts"
        ]
    }
}