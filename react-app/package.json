{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "@kbase/ui-components": "0.5.18",
    "@kbase/ui-lib": "0.4.12",
    "@react-leaflet/core": ">=1.0.0 <1.1.0 || ^1.1.1",
    "@types/leaflet": "1",
    "@types/react": "17",
    "@types/react-dom": "17",
    "@types/react-redux": "7",
    "@types/react-router-dom": "5",
    "ajs": "1.3.3",
    "antd": "4.18.4",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "leaflet": "1.7.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-intl": "5.24.3",
    "react-leaflet": ">=3.1.0 <3.2.0 || ^3.2.1",
    "react-redux": "7.2.6",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@craco/craco": "6.2.0",
    "@jedmao/redux-mock-store": "3.0.5",
    "@storybook/addon-actions": "6.3.4",
    "@storybook/addon-essentials": "6.5.13",
    "@storybook/addon-links": "6.3.4",
    "@storybook/node-logger": "6.3.4",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.5.13",
    "@testing-library/dom": "8.1.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.0.0",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26",
    "@types/node": "14",
    "babel-loader": "8.1.0",
    "babel-plugin-import": "1.13.3",
    "canvas": "2.8.0",
    "craco-antd": "git://github.com/eapearson/craco-antd#master",
    "less": "4.1.1",
    "less-loader": "7.3.0",
    "nodemon": "2.0.10",
    "redux-mock-store": "1.5.4",
    "storybook-preset-craco": "0.0.4",
    "typescript": "4.6.4",
    "webpack": "4.44.2"
  },
  "scripts": {
    "start": "nodemon -w ./config-overrides.js --ext ts,tsx,css,yml,yaml,json --exec 'craco start'",
    "build": "craco build",
    "test": "craco test",
    "test-coverage": "craco test --coverage --watchAll=false",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app",
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "homepage": "./",
  "proxy": "https://ci.kbase.us",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/index.tsx",
      "!**/*.stories.*"
    ],
    "globalSetup": "./scripts/setupJest.js"
  }
}
